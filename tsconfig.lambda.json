{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "outDir": "lambda-build/dist",
    "rootDir": "lambda-build",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "skipLibCheck": true,
    "sourceMap": false
  },
  "include": ["lambda-build/**/*.ts"],
  "exclude": ["node_modules", "**/*.test.ts"]
} 